<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="387.0" prefWidth="396.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_1.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" alignment="CENTER" contentDisplay="CENTER" prefHeight="87.0" prefWidth="389.0" text="BRIARTHORN" textFill="#ff1a1a" underline="true">
      <font>
         <Font name="Arial Black" size="24.0" />
      </font></Label>
    <Button alignment="CENTER" graphicTextGap="2.0" onAction="#onHelloButtonClick" prefHeight="77.0" prefWidth="173.0" style="-fx-background-color: aqua;" text="KLIKNII" textAlignment="CENTER" textFill="#ca16d7" textOverrun="CENTER_ELLIPSIS">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
      <font>
         <Font name="Berlin Sans FB" size="18.0" />
      </font></Button>
</VBox>
